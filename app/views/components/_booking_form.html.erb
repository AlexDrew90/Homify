<%= simple_form_for [@match, @viewing] do |f| %>
<div class="form-inputs">
  <%= f.input :date, as: :date, html5: true%>
  <%= f.input :start_time, :label => "start time (all slots are 15mins)", as: :time, html5: true %>
</div>
<div class="booking-submit">
  <%=  f.button :submit, "book viewing" %>
</div>
<% end %>
